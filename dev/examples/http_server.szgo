		"Generate a Mandelbrot": {
			"part": {
				"imports": [
					"\"image\"",
					"\"image/color\"",
					"\"image/png\"",
					"\"math/cmplx\"",
					"\"github.com/google/shenzhen-go/dev/parts\""
				],
				"head": [
					"const tileW, tileH = 320, 240",
					"const depth = 25"
				],
				"body": [
					"for req := range requests {",
					"\t// Serve a PNG",
					"",
					"\treq.Header().Set(\"Content-Type\", \"image/png\")",
					"\treq.WriteHeader(http.StatusOK)",
					"",
					"\timg := image.NewRGBA(image.Rect(0, 0, tileW, tileH))",
					"",
					"\tfor i := 0; i \u003c tileW; i++ {",
					"\t\tfor j := 0; j \u003c tileH; j++ {",
					"\t\t\tc := complex(float64(i-tileW/2), float64(j-tileH/2))",
					"\t\t\tc *= (4 + 0i) / (tileW + 0i)",
					"\t\t\tz := 0i",
					"\t\t\tcol := color.Black",
					"\t\t\tfor k := 0; k \u003c depth; k++ {",
					"\t\t\t\tz = z*z + c",
					"\t\t\t\tif cmplx.Abs(z) \u003e 2 {",
					"\t\t\t\t\tcol = color.White",
					"\t\t\t\t\tbreak",
					"\t\t\t\t}",
					"\t\t\t}",
					"\t\t\timg.Set(i, j, col)",
					"\t\t}",
					"\t}",
					"",
					"\tpng.Encode(req, img)",
					"\treq.Close()",
					"}"
				],
				"tail": [
					""
				],
				"pins": {
					"requests": {
						"type": "*parts.HTTPRequest",
						"dir": "in"
					}
				}
			},
			"part_type": "Code",
			"comment": "Generates a Mandelbrot fractal",
			"enabled": true,
			"wait": true,
			"multiplicity": "N",
			"x": 26,
			"y": 566,
			"connections": {
				"requests": "channel7"
			}
		},
